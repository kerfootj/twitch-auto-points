{
	"name": "Twitch Auto Channel Points",
	"description": "Automatically collects Twitch Channel Points",
	"version": "1.1.0",
	"manifest_version": 3,
	"content_scripts": [
		{
			"matches": ["*://*.twitch.tv/*"],
			"js": ["points.js"]
		}
	],
	"icons": {
		"128": "icon_128.png",
		"48": "icon_48.png"
	}
}
